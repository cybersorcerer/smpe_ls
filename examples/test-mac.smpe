/* ++MAC Test File
Testing ADD/UPDATE and DELETE modes for ++MAC MCS statement
Valid ++MAC ADD with required DISTLIB
*/
++MAC(MYMAC) DISTLIB(AMACLIB) .
MYMAC MACRO
	LA	R8,1
	ST	R8,MY_REG
	MEND
/* laskdöaskdöla */
++MAC(MYMAC2)
  DISTLIB(AMACLIB)
  SYSLIB(ASYSLIB)
  MALIAS(MAC2A,MAC2B).

/* Valid ++MAC with ASSEM */
++MAC(MYMAC3)
  DISTLIB(AMACLIB)
  ASSEM(MOD1,MOD2)
  DISTMOD(AMODLIB)
  DISTSRC(ASRCLIB).

/* Valid ++MAC with FROMDS */
++MAC(MYMAC4)
  DISTLIB(AMACLIB)
  FROMDS(DSN(MY.DATA.SET) NUMBER(1) VOL(WORK01)).

/* Valid ++MAC with RELFILE */
++MAC(MYMAC5)
  DISTLIB(AMACLIB)
  RELFILE(1).

/* Valid ++MAC with TXLIB */
++MAC(MYMAC6)
  DISTLIB(AMACLIB)
  TXLIB(ATXLIB).

/* Valid ++MAC with SSI */
++MAC(MYMAC7)
  DISTLIB(AMACLIB)
  SSI(12345678).

/* Valid ++MAC with PREFIX and RMID */
++MAC(MYMAC8)
  DISTLIB(AMACLIB)
  PREFIX(ABC)
  RMID(PTF0001)
  UMID(PTF0002,PTF0003).

/* Valid ++MAC with VERSION */
++MAC(MYMAC9)
  DISTLIB(AMACLIB)
  VERSION(FMID001,FMID002).

/* Valid ++MAC DELETE with DISTLIB */
++MAC(OLDMAC) DELETE DISTLIB(AMACLIB).

/* Valid ++MAC DELETE without DISTLIB */
++MAC(OLDMAC2) DELETE.

/* Valid ++MAC DELETE with VERSION */
++MAC(OLDMAC3) DELETE VERSION(FMID001).

/* Invalid: ++MAC without DISTLIB (should show warning/error in non-DELETE mode) */
++MAC(BADMAC) .

/* Invalid: ++MAC with DELETE and ASSEM (mutually exclusive) */
++MAC(BADMAC2) DELETE ASSEM(MOD1).

/* Invalid: ++MAC with DELETE and DISTMOD (mutually exclusive) */
